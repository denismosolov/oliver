# чтобы отличать лоты от акций
entity OperationUnit:
    values:
        lot:
            %lemma
            лот
            # ошибки распознавания
            %exact
            лото
            лотос
            котов
            сотов
            модов
            мотов
            мотор
            вот
            лофт
        share:
            %lemma
            акция

# чтобы различать покупку и продажу
entity OperationType:
    values:
        buy:
            %lemma
            купи
        sell:
            %lemma
            продай

